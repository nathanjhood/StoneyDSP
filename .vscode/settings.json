		"istream": "cpp",
		"limits": "cpp",
		"new": "cpp",
		"numbers": "cpp",
		"ostream": "cpp",
		"stdexcept": "cpp",
		"streambuf": "cpp",
		"cinttypes": "cpp",
		"typeinfo": "cpp",
		"sstream": "cpp",
		"ctime": "cpp",
		"chrono": "cpp",
		"condition_variable": "cpp",
		"ratio": "cpp",
		"mutex": "cpp",
		"semaphore": "cpp",
		"stop_token": "cpp",
		"thread": "cpp",
		"cfenv": "cpp",
		"complex": "cpp",
