{
	"makefile.configurations": [
		{
			"name": "Default",
			"makeArgs": [
				"BUILD_CORE=1",
				"BUILD_TEST=1",
				"DEBUG=1"
			]
		}
	],
	"makefile.launchConfigurations": [
		{
			"cwd": "${workspaceFolder}/build/bin",
			"binaryPath": "${workspaceFolder}/build/bin/main",
			"binaryArgs": [],
			"MIMode": "gdb",
			"miDebuggerPath": "${workspaceFolder}/share/gdbinit",
			"stopAtEntry": false,
			"symbolSearchPath": "${workspaceFolder}/include"
		},
		{
			"cwd": "/home/stoneydsp/dev/stoneydsp/StoneyDSP/build/test",
			"binaryPath": "/home/stoneydsp/dev/stoneydsp/StoneyDSP/build/test/main",
			"binaryArgs": []
		}
	],
	"cmake.debugConfig": {
		"cwd": "${workspaceFolder}",
		"symbolSearchPath": "${workspaceFolder}/include",
		"miDebuggerPath": "${workspaceFolder}/share/gdbinit",
		"MIMode": "gdb",
		"console": "newExternalWindow",
		"stopAtEntry": false,
		"logging": {
			"engineLogging": false,
			"exceptions": true,
			"moduleLoad": true,
			"trace": false,
			"traceResponse": false,
			"programOutput": true
		}
	},
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"editor.trimAutoWhitespace": true,
	"editor.insertSpaces": true,
	"editor.rulers": [
		80,
		160
	],
	"editor.formatOnSave": true,
	"json.format.enable": true,
	"json.validate.enable": true,
	"[json]": {
		"editor.quickSuggestions": {
			"strings": true
		},
		"editor.suggest.insertMode": "replace",
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"makefile.configureOnOpen": true,
	"makefile.compileCommandsPath": "${workspaceFolder}/build/compile_commands_make.json",
	"makefile.makeDirectory": "${workspaceFolder}",
	"makefile.makefilePath": "${workspaceFolder}/Makefile",
	"makefile.makePath": "make",
	"makefile.buildBeforeLaunch": true,
	"makefile.clearOutputBeforeBuild": true,
	"makefile.configurationCachePath": "${workspaceFolder}/build/MakeFiles/configurationCache.log",
	"makefile.extensionOutputFolder": "${workspaceFolder}/build/MakeFiles",
	"cmake.cmakePath": "cmake",
	"cmake.cpackPath": "cpack",
	"cmake.ctestPath": "ctest",
	"cmake.enableAutomaticKitScan": true,
	"cmake.configureOnOpen": false,
	"cmake.configureOnEdit": false,
	"cmake.buildBeforeRun": true,
	"cmake.clearOutputBeforeBuild": true,
	"cmake.deleteBuildDirOnCleanConfigure": false,
	"cmake.skipConfigureIfCachePresent": true,
	"cmake.automaticReconfigure": true,
	"cmake.exportCompileCommandsFile": true,
	"cmake.useCMakePresets": "always",
	"cmake.sourceDirectory": "${workspaceFolder}",
	"cmake.buildDirectory": "${workspaceFolder}/build",
	"cmake.installPrefix": "${workspaceFolder}/install",
	"cmake.copyCompileCommands": "${workspaceFolder}/build/compile_commands_cmake.json",
	"cmake.loadCompileCommands": true,
	"cmake.setBuildTypeOnMultiConfig": true,
	"cmake.preferredGenerators": [
		"Ninja",
		"Ninja Multi-Config"
	],
	"doxdocgen.generic.briefTemplate": "@brief {text}",
	"doxdocgen.generic.dateTemplate": "@date {date}",
	"doxdocgen.file.fileTemplate": "@file {name}",
	"doxdocgen.file.copyrightTag": [
		"@copyright Copyright (c) {year}"
	],
	"doxdocgen.file.versionTag": "@version 0.1",
	"doxdocgen.file.fileOrder": [
		"file",
		"author",
		"brief",
		"version",
		"date",
		"empty",
		"copyright",
		"empty",
		"custom"
	],
	// "doxdocgen.file.customTag": [],
	"doxdocgen.c.factoryMethodText": "Create a {name} object",
	"doxdocgen.generic.dateFormat": "YYYY-MM-DD",
	"doxdocgen.generic.boolReturnsTrueFalse": true,
	// "doxdocgen.generic.filteredKeywords": [],
	// "doxdocgen.generic.customTags": [],
	"doxdocgen.generic.commandSuggestion": true,
	"doxdocgen.generic.commandSuggestionAddPrefix": false,
	"doxdocgen.generic.generateSmartText": true,
	"doxdocgen.generic.splitCasingSmartText": true,
	"doxdocgen.c.getterText": "Get the {name} object",
	"doxdocgen.c.setterText": "Set the {name} object",
	"doxdocgen.cpp.ctorText": "Construct a new {name} object",
	"doxdocgen.cpp.dtorText": "Destroy the {name} object",
	"doxdocgen.cpp.tparamTemplate": "@tparam {param} ",
	"doxdocgen.generic.useGitUserName": true,
	"doxdocgen.generic.useGitUserEmail": true,
	"doxdocgen.generic.paramTemplate": "@param {param} ",
	"doxdocgen.generic.returnTemplate": "@return {type} ",
	"doxdocgen.generic.includeTypeAtReturn": true,
	"doxdocgen.generic.authorEmail": "you@domain.com",
	"doxdocgen.generic.authorName": "your name",
	"doxdocgen.generic.authorTag": "@author {author} ({email})",
	"doxdocgen.generic.linesToGet": 20,
	"doxdocgen.generic.order": [
		"brief",
		"empty",
		"tparam",
		"param",
		"return",
		"custom"
	],
	"doxdocgen.c.triggerSequence": "/**",
	"doxdocgen.c.firstLine": "/**",
	"doxdocgen.c.commentPrefix": " * ",
	"doxdocgen.c.lastLine": " */",
	"C_Cpp.commentContinuationPatterns": [
		"/**"
	],
	"C_Cpp.doxygen.generatedStyle": "/**",
	"C_Cpp.doxygen.generateOnType": true,
	"C_Cpp.doxygen.sectionTags": [
		"attention",
		"tparam",
		"param",
		"result",
		"returns",
		"retval",
		"exception",
		"deprecated",
		"warning",
		"note"
	],
	"C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
	"C_Cpp.formatting": "clangFormat",
	"C_Cpp.autoAddFileAssociations": true,
	"C_Cpp.clang_format_style": "file:${workspaceFolder}/.clang-format",
	"C_Cpp.clang_format_fallbackStyle": "GNU",
	"C_Cpp.clang_format_sortIncludes": true,
	"C_Cpp.codeAnalysis.clangTidy.enabled": true,
	"C_Cpp.codeAnalysis.clangTidy.codeAction.formatFixes": true,
	"C_Cpp.codeAnalysis.clangTidy.headerFilter": null,
	"C_Cpp.codeAnalysis.clangTidy.fallbackConfig": "{BasedOnStyle: gnu, Checks: '-*,clang-analyzer-*'}",
	"C_Cpp.autocomplete": "default",
	"C_Cpp.default.cStandard": "c11",
	"C_Cpp.default.cppStandard": "c++11",
	"C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
	"C_Cpp.default.compilerPath": "/usr/bin/gcc",
	"C_Cpp.default.mergeConfigurations": true,
	"C_Cpp.default.includePath": [
		"${workspaceFolder}/include"
	],
	"C_Cpp.files.exclude": {
		"**/.vscode": true,
		"**/.vs": true
	},
	"files.associations": {
		"**.d.ts": "typescript",
		"*.css": "tailwindcss",
		"**.bash**": "shellscript",
		"**.cmake": "cmake",
		".editorconfig": "editorconfig",
